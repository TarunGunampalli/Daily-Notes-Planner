{
    "manifest_version": 2,
  
    "name": "Daily Tasks",
    "description": "A simple note taking extension",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icons/icon.png",
     "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://unpkg.com/react@16/umd/react.production.min.js https://unpkg.com/react-dom@16/umd/react-dom.production.min.js; object-src 'self'",
    "permissions": [
     "activeTab"
     ]
  }